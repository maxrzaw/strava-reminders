{
  "devDependencies": {
    "prettier": "^3.0.3",
    "prettier-plugin-go-template": "^0.0.15",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "tailwindcss": "^3.3.5"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-go-template",
      "prettier-plugin-tailwindcss"
    ],
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "go-template"
        }
      }
    ]
  }
}
